{
  "expo": {
    "name": "pet",
    "slug": "pet",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/pata.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/raca-pura.png",
        "backgroundColor": "#ffffff",
        "versionCode": "2"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.paulo_s19.pet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "cd93ecac-6c78-4b94-995d-3ef3136842a3"
      }
    }
  }
}
